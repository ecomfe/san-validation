  "license": "MIT",
  "readme": "README",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "edp": "^1.1.1",
    "edp-test": "^1.1.1",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3"
  }
